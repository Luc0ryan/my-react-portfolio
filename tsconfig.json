{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "moduleResolution": "bundler",
      "strict": true,
      "noImplicitAny": false,
      "allowJs": true,
      "checkJs": false,
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": { "@/*": ["src/*"] },
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src", "vite-env.d.ts"]
  }
  